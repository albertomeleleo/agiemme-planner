{
  "name": "agiemme-planner",
  "version": "1.0.0",
  "description": "Personal Growth Planner - OKR Management Application",
  "private": true,
  "scripts": {
    "docker:check-ports": "bash scripts/check-ports.sh",
    "docker:up": "npm run docker:check-ports && docker compose up",
    "docker:up:build": "npm run docker:check-ports && docker compose up --build",
    "docker:up:d": "npm run docker:check-ports && docker compose up -d",
    "docker:down": "docker compose down",
    "docker:clean": "bash scripts/cleanup.sh",
    "docker:clean:volumes": "bash scripts/cleanup.sh --volumes",
    "docker:clean:all": "bash scripts/cleanup.sh --all --yes",
    "docker:logs": "bash scripts/logs.sh",
    "docker:logs:follow": "bash scripts/logs.sh --follow",
    "docker:logs:backend": "bash scripts/logs.sh backend",
    "docker:logs:frontend": "bash scripts/logs.sh frontend",
    "docker:logs:db": "bash scripts/logs.sh postgres",
    "docker:migrate:up": "bash scripts/migrate.sh up",
    "docker:migrate:down": "bash scripts/migrate.sh down",
    "docker:migrate:status": "bash scripts/migrate.sh status",
    "docker:backup": "bash scripts/backup.sh",
    "docker:backup:volume": "bash scripts/backup.sh --volume",
    "docker:restore": "bash scripts/restore.sh",
    "docker:ps": "docker compose ps",
    "docker:restart": "docker compose restart",
    "docker:restart:backend": "docker compose restart backend",
    "docker:restart:frontend": "docker compose restart frontend"
  },
  "keywords": ["okr", "personal-growth", "goals", "docker"],
  "author": "Agiemme",
  "license": "UNLICENSED"
}
